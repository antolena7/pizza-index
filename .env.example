# Copy this file to .env and fill in your actual API keys

# Google Places API Key (required for real-time pizza data)
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# News API Key (required for live news feed)
NEWS_API_KEY=your_news_api_key_here

# New York Times API Key (required for NYT news)
NYT_API_KEY=your_nyt_api_key_here

# Database URL (optional, defaults to SQLite)
DATABASE_URL=sqlite:///pizza_index.db

# Session Secret (optional, has default)
SESSION_SECRET=your_secret_key_here